{
    "name": "trigger censor",
    "version": "1.0",
    "manifest_version": 2,
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Trigger Censor"
    }
}